{
  "name": "ryusoudev",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "npm run lint:es && npm run lint:style",
    "lint:fix": "npm run lint:es:fix",
    "lint:es": "eslint 'src/**/*.{js,jsx,ts,tsx}'",
    "lint:es:fix": "eslint --fix 'src/**/*.{js,jsx,ts,tsx}'",
    "lint:style": "stylelint 'src/**/*.css'",
    "lint:style:fix": "stylelint --fix 'src/**/*.css'",
    "lint:conflict": "eslint --print-config .eslintrc.js | eslint-config-prettier-check",
    "format": "prettier --write 'src/**/*.{js,jsx,ts,tsx}'",
    "typecheck": "tsc --noEmit",
    "prepare": "husky install",
    "lint-staged": "lint-staged"
  },
  "lint-staged": {
    "*.{js,ts,jsx,tsx}": [
      "yarn format",
      "yarn lint:fix"
    ]
  },
  "dependencies": {
    "@vanilla-extract/css": "^1.1.2",
    "dayjs": "^1.10.6",
    "microcms-js-sdk": "^1.2.0",
    "next": "11.0.1",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-icons": "^4.2.0",
    "react-intersection-observer": "^8.32.0",
    "react-query": "^3.19.0"
  },
  "devDependencies": {
    "@next/eslint-plugin-next": "11.0.2-canary.19",
    "@types/eslint": "7.28.0",
    "@types/eslint-plugin-prettier": "^3.1.0",
    "@types/node": "16.4.0",
    "@types/prettier": "^2.3.2",
    "@types/react": "17.0.14",
    "@types/react-dom": "17.0.9",
    "@types/stylelint": "^13.13.1",
    "@typescript-eslint/eslint-plugin": "^4.28.4",
    "@typescript-eslint/parser": "^4.28.4",
    "@vanilla-extract/babel-plugin": "^1.0.1",
    "@vanilla-extract/webpack-plugin": "^1.0.3",
    "css-loader": "^6.2.0",
    "eslint": "^7.31.0",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-config-next": "^11.0.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.23.4",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-prettier": "^3.4.0",
    "eslint-plugin-react": "^7.24.0",
    "eslint-plugin-react-hooks": "^4.2.0",
    "husky": "^7.0.0",
    "lint-staged": "^11.1.0",
    "mini-css-extract-plugin": "^2.1.0",
    "prettier": "^2.3.2",
    "stylelint": "^13.13.1",
    "stylelint-config-recess-order": "^2.4.0",
    "stylelint-config-standard": "^22.0.0",
    "stylelint-order": "^4.1.0",
    "typescript": "4.3.5",
    "webpack": "^5.46.0"
  }
}
